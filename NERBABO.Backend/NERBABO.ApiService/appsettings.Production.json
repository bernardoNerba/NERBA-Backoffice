{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "Key": "${JWT_KEY}",
    "ExpiresInDays": 1,
    "Issuer": "${JWT_ISSUER}",
    "ClientUrl": "${CLIENT_URL}"
  },
  "Admin": {
    "username": "${ADMIN_USERNAME}",
    "email": "${ADMIN_EMAIL}",
    "password": "${ADMIN_PASSWORD}"
  },
  "CORS": {
    "AllowedOrigins": [
      "${CLIENT_URL}",
      "http://${SERVER_IP}:4200",
      "https://${SERVER_IP}:4200"
    ]
  }
}
